{
	# debug
}

$WEB_EDITOR_HOSTNAME {
	reverse_proxy localhost:8000
}

$WEB_EDITOR_APPS_HOSTNAME {
	rewrite /v1/projects/start/{labels.2}/$WEB_EDITOR_APP_SECRET
	reverse_proxy localhost:8000
}

(proxy_error_handling) {
	handle_errors {
		rewrite * /{err.status_code}.html 
		root * ./src/caddy/html/
		file_server
	}	
}
